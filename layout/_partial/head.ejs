<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <meta name="description" content="<%= config.description %>">
  <meta name="keywords" content="<%= config.keywords %>">
  <meta name="author" content="<%= config.author %>">

  <meta name="generator" content="hexo">
  <meta name="theme-color" content="#fafafa">

  <meta name="color-scheme" content="light dark">

  <meta name="creator" content="<%= theme.creator %>">
  <meta name="publisher" content="<%= theme.publisher %>">

  <%# test %>
  <title><%= page.title %></title>
  <%# icon %>
  <%- favicon_tag(theme.icon) %>

  <%# css link - stylus %>
  <%- css('css/index.css') %>

  <% if(theme.comments.gitalk.enable) { %>
    <link rel="stylesheet" href="<%= theme.CDN.gitalkStyle %>">
  <% } %>
  <% if(theme.comments.gitment.enable) { %>
    <link rel="stylesheet" href="<%= theme.CDN.gitmentStyle %>">
  <% } %>
  <% if(theme.comments.vssue.enable) { %>
    <link rel="stylesheet" href="<%= theme.CDN.vssueStyle %>">
  <% } %>
</head>